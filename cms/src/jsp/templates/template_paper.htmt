		<script type="text/javascript">
            function writeCode(linkText, fileName, caption) {
                    iFigure = fileName.indexOf("%FIG:FIGURE");
                    if (iFigure == -1) {
                            var startHREF="<iframe src=\"../plots/view.jsp?menu=no&type=plot&get=data&filename="+fileName+"\" width=\"100%\" height=\"600\">"
                            var endHREF="</iframe>";
                            var code=startHREF+linkText+endHREF+"<br /> "+caption;
                            document.write(code);
                    }
            }
		</script>
