<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE root SYSTEM "ogre.dtd">

<root type="Monte Carlo Simulation">

  <dataset name="mc09" location="/home/ogre/mcdata">
    <database>
      <field name="run" />
      <field name="nevents" />
      <field name="dataset" />
      <field name="filename" />
    </database>
  </dataset>

  <triggers level="0">
    <trigger name="signal" id="sig" description="Z -> ee Signal events"     level="0"/>
    <trigger name="backgd" id="bkg" description="TTbar and QCD backgrounds" level="0"/>
  </triggers>

  <tree id="1" name="Events" level="0">
    <branch id="1"  name="patElectrons_selectedLayer1Electrons__PAT.obj" level="0">
      <leaf id="1"  name="pt_" 
	    title="Electron Pt"   labely="Number of Events" labelx="P_t (GeV)" level="0"/>
      <leaf id="2"  name="phi_"
            title="Electron Phi"   labely="Number of Events" labelx="Phi (rad)" level="0"/>
      <leaf id="3"  name="eta_"                      
	    title="Electron Eta"   labely="Number of Events" labelx="Eta" level="0"/>
      <leaf id="4"  name="superClusterEnergy_"       
	    title="Electron E"   labely="Number of Events" labelx="E_dep (GeV)" level="0"/>
      <leaf id="5"  name="vertex_.fCoordinates.fX_"  
	    title="Electron Vertex X"   labely="Number of Events" labelx="X_v (cm)" level="0"/>
      <leaf id="6"  name="vertex_.fCoordinates.fY"   
	    title="Electron Vertex Y"   labely="Number of Events" labelx="Y_v (cm)" level="0"/>
      <leaf id="7"  name="vertex_.fCoordinates.fZ"   
	    title="Electron Vertex Z"   labely="Number of Events" labelx="Z_v (cm)" level="0"/>
    </branch>
    <branch id="2"  name="patPhotons_selectedLayer1Photons__PAT.obj" level="0">
      <leaf id="8"  name="pt_"
	    title="Photon Pt"   labely="Number of Events" labelx="P_t (GeV)" level="0"/>
      <leaf id="9"  name="phi_"
            title="Photon Phi"   labely="Number of Events" labelx="Phi (rad)" level="0"/>
      <leaf id="10"  name="eta_"                      
	    title="Photon Eta"   labely="Number of Events" labelx="Eta" level="0"/>
      <leaf id="11" name="superClusterEnergy_"       
	    title="Photon E"   labely="Number of Events" labelx="E_dep (GeV)" level="0"/>
      <leaf id="12" name="vertex_.fCoordinates.fX_"  
	    title="Photon Vertex X"   labely="Number of Events" labelx="X_v (cm)" level="0"/>
      <leaf id="13" name="vertex_.fCoordinates.fY"   
	    title="Photon Vertex Y"   labely="Number of Events" labelx="Y_v (cm)" level="0"/>
      <leaf id="14" name="vertex_.fCoordinates.fZ"   
	    title="Photon Vertex Z"   labely="Number of Events" labelx="Z_v (cm)" level="0"/>
    </branch>
  </tree>
</root>
