<table id="list">
	<tr>
		<th>Type</th><th>File count</th><th>Event count</th><th>Mode</th><th>Est. time</th><th>Start delay</th>
		<c:forEach var="a" items="${l}">
			<tr>
				<td>${a.type}</td>
				<td class="numeric"><a href="#" title="${a.files}">${a.fileCount}</a></td>
				<td class="numeric">${a.events}</td>
				<td>${a.mode}</td>
				<td class="numeric">${a.etime}</td>
				<td class="numeric">${a.cDelay/10}</td>
			</tr>
		</c:forEach>
	</tr>
</table>