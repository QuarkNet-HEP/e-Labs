<html>
	<head>
		<title>Poster</title>
		<link rel="stylesheet" type="text/css" href="../css/poster.css" />

		<script language="JavaScript">
			self.name="Poster_main";
			function showFigure(url) {
				window.open(url, "graph", "width=820,height=750,scrollbars=yes,toolbar=no,menubar=no,status=yes,resizable=yes");
			}

			function writeCode(linkText, fileName, caption) {
   				iFigure = fileName.indexOf("%FIG:FIGURE");
   				if (iFigure == -1) {
   					startHREF="<LI><A HREF=\"javascript:showFigure('../plots/view.jsp?menu=no&type=plot&get=data&filename="+fileName+"')";
   					endHREF="</A>";
   					quoteAngle="\">";
       				code=startHREF+quoteAngle+linkText+ endHREF+": "+caption;
   					document.write(code);
   				}
   			}
		</script>
	</head>

	<body class="poster_body">
		<center>
			<table width="650" class="poster_main_table" cellspacing="6px">
				<tr>
      				<td colspan="2" id="poster_header"> 
        				<table width="100%">
        					<tr><td align="center" class="poster_title" valign="top">%WORDS:TITLE%</td></tr>
           					<tr><td align="center" class="poster_subtitle">%WORDS:SUBTITLE%</td></tr>
           					<tr><td align="center" class="poster_subsubtitle">%WORDS:AUTHORS%</td></tr>
           					<tr><td align="center" class="poster_subsubtitle">%WORDS:DATE%</td></tr>
           				</table>
					</td>
				</tr>
				<tr>
					<td colspan="2" valign="top" class="poster_detail_frame">
						<a name="bibliography"></a>
						<table width="632" class="poster_detail_head">
							<tr>
								<td class="poster_detail_body">
									<br>
									<p class="poster_detail_subhead">Abstract</p>
									<p>
										%PARA:ABSTRACT%
									</p>
									<br>
									<p>&nbsp;</p>
								</td>
								<td></td>
							</tr>
						</table>
					</td>
				</tr>
				<tr>
					<td valign="TOP" class="poster_detail_frame" width="310">
						<a name="authors"> </a>
						<table class="poster_detail_head" width="302">
							<a name="abstract"></a>
							<tr>
								<td align="left" class="poster_detail_body">
									<p class="poster_detail_subhead">Introduction</p>
									<p>
										%PARA:INTRODUCTION%
									</p>&nbsp;
								</td>
								<td></td>
							</tr>
						</table>
					</td>
					<td width="310" valign="top" class="poster_detail_frame">
						<a name="materials"></a>
						<table class="poster_detail_head" width="302">
							<tr>
								<td class="poster_detail_body">
									<br>
									<p class="poster_detail_subhead">Procedures</p>
									<p>
										%PARA:PROCEDURE%
									</p>&nbsp;
								</td>
								<td></td>
							</tr>
						</table>
					</td>
				</tr>
				<tr>
					<td width="310" valign="top" class="poster_detail_frame">
						<a name="results"></a>
						<table class="poster_detail_head" width="302">
							<tr>
								<td class="poster_detail_body">
									<br>
									<p class="poster_detail_subhead">Results</p>
									<p>
										%PARA:RESULTS%
									</p>
									<table>
										<tr>
											<td class="poster_figure_name">
												<ul>
<script language="JAVASCRIPT">writeCode("Figure 1","%FIG:FIGURE1%","%WORDS:CAPTION1%")</script>
<script language="JAVASCRIPT">writeCode("Figure 2","%FIG:FIGURE2%","%WORDS:CAPTION2%")</script>
<script language="JAVASCRIPT">writeCode("Figure 3","%FIG:FIGURE3%","%WORDS:CAPTION3%")</script>
<script language="JAVASCRIPT">writeCode("Figure 4","%FIG:FIGURE4%","%WORDS:CAPTION4%")</script>
<script language="JAVASCRIPT">writeCode("Figure 5","%FIG:FIGURE5%","%WORDS:CAPTION5%")</script>
<script language="JAVASCRIPT">writeCode("Figure 6","%FIG:FIGURE6%","%WORDS:CAPTION6%")</script>
<script language="JAVASCRIPT">writeCode("Figure 7","%FIG:FIGURE7%","%WORDS:CAPTION7%")</script>
<script language="JAVASCRIPT">writeCode("Figure 8","%FIG:FIGURE8%","%WORDS:CAPTION8%")</script>
<script language="JAVASCRIPT">writeCode("Figure 9","%FIG:FIGURE9%","%WORDS:CAPTION9%")</script>
<script language="JAVASCRIPT">writeCode("Figure 10","%FIG:FIGURE10%","%WORDS:CAPTION10%")</script>
												</ul>
											</td>
										</tr>
									</table>
									<p>&nbsp;</p>
								</td>
								<td></td>
							</tr>
						</table>
					</td>
					<td width="310" valign="top" class="poster_detail_frame">
						<a name="discussion"></a>
						<table width="302" class="poster_detail_head">
							<tr>
								<td class="poster_detail_body">
									<br>
									<p class="poster_detail_subhead">Discussions & Conclusions</p>
									<p>
										%PARA:CONCLUSION%
									</p>
									<br>
									<p>&nbsp;</p>
								</td>
								<td></td>
							</tr>
						</table>
					</td>
				</tr>
				<tr>
					<td colspan="2" valign="top" class="poster_detail_frame">
						<a name="bibliography"></a>
						<table width="632" class="poster_detail_head">
							<tr>
								<td class="poster_detail_body">
									<br>
									<p class="poster_detail_subhead">Bibliography</p>
									<p>
										%PARA:BIBLIOGRAPHY%
									</p>
									<br>
									<p>&nbsp;</p>
								</td>
								<td></td>
							</tr>
						</table>
					</td>
				</tr>
			</table>
		</center>
	</body>
</html>

