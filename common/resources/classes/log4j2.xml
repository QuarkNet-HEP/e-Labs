<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
  <Loggers>
    <Root level="warn">
      <AppenderRef ref="CONSOLE" />
    </Root>
    <Logger name="swift" level="debug">
    </Logger>
    <Logger name="org.globus.cog.abstraction.impl.common" level="warn">
    </Logger>
    <Logger name="org.apache.axis.utils.JavaUtils" level="error">
    </Logger>
  </Loggers>
  <Appenders>
    <Appender type="Console" name="CONSOLE">
      <Layout type="PatternLayout" pattern="%d{yyyy-MM-dd HH:mm:ss,SSSZZZZZ}" />
    </Appender>
    <Appender type="Console" name="CONSOLE-C">
      <Layout type="PatternLayout" pattern="%-5p [%c] %x - %m%n" />
    </Appender>
  </Appenders>
</Configuration>
